   "source": [
    "def simulation(n):\n",
    "    P = {team : 0 for team in set(T['DOM'])}\n",
    "    for k in range(n):\n",
    "        for e1, e2, EX, EY in zip(T['DOM'], T['EXT'], T['EX'], T['EY']):\n",
    "            x = nb_buts(EX)\n",
    "            y = nb_buts(EY)\n",
    "            if x > y :\n",
    "                P[e1] = P[e1] + 3\n",
    "            elif x == y:\n",
    "                P[e1] = P[e1] + 1\n",
    "                P[e2] = P[e2] + 1\n",
    "            else :\n",
    "                P[e2] = P[e2] + 3       \n",
    "    return {equipe : P[equipe]/n for equipe in P }"
   ]
